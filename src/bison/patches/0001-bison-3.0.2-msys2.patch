index d477fb4..32f0c5c 100644
--- a/m4/realloc.m4
+++ b/m4/realloc.m4
@@ -30,7 +30,7 @@ AC_DEFUN([_AC_FUNC_REALLOC_IF],
        [case "$host_os" in
