From b6d327c74b29a26669d847f8fa46ba60350a441c Mon Sep 17 00:00:00 2001
From: Hiroshi Shirosaki <h.shirosaki@gmail.com>
Date: Wed, 9 Jul 2014 15:25:31 +0900
Subject: [PATCH 38/41] Compile insn-recog.o with -O1 to fix compile error

---
 gcc/Makefile.in | 3 +++
 1 file changed, 3 insertions(+)

diff --git a/gcc/Makefile.in b/gcc/Makefile.in
index 9241cf7..a8c98bc 100644
--- a/gcc/Makefile.in
+++ b/gcc/Makefile.in
@@ -1055,6 +1055,9 @@ INCLUDES = -I. -I$(@D) -I$(srcdir) -I$(srcdir)/$(@D) \
 	   $(CPPINC) $(GMPINC) $(DECNUMINC) $(BACKTRACEINC) \
 	   $(CLOOGINC) $(ISLINC)
 
+insn-recog.o:
+	$(COMPILER) -c $(ALL_COMPILERFLAGS:-O2=-O1) $(ALL_CPPFLAGS) $< $(OUTPUT_OPTION)
+
 .c.o:
 	$(COMPILER) -c $(ALL_COMPILERFLAGS) $(ALL_CPPFLAGS) $< $(OUTPUT_OPTION)
 
